.container{
    width: 100%;
    height: 100%;
    background: #FFF;
    justify-content: flex-start;
    padding: 0px;
}
.search-wrap{
    width: 86%;
    height: 100rpx;
    background: #EFEFF4;
    border: none;
    border-radius: 10rpx;
    padding: 0px 100rpx 0rpx 28rpx;
    margin-top: 50%;
    position: relative;
    box-sizing: border-box;
}
.search-wrap input{
    width: 100%;
    height: 100%;
    font-size: 34rpx;
}
.search-wrap image{
    width: 60rpx;
    height: 60rpx;
    position: absolute;
    right: 20rpx;
    top: 20rpx;
}